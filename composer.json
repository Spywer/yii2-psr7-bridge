{
    "name": "spywer/yii2-psr7-bridge",
    "description": "A PSR-7 bridge and PSR-15 adapter for Yii2 web applications (PHP 8.5 compatible)",
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Charles R. Portwood II",
            "email": "charlesportwoodii@erianna.com"
        },
        {
            "name": "Spywer"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.5",
        "yiisoft/yii2": "^2.0.53",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "laminas/laminas-diactoros": "^3.8"
    },
    "require-dev": {
        "samdark/yii2-psr-log-target": "^1.0",
        "monolog/monolog": "~2.1.0",
        "squizlabs/php_codesniffer": "^3.2",
        "phpunit/phpunit": "^9.0",
        "spiral/roadrunner": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "yii\\Psr7\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "yii\\Psr7\\tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-compose": true,
            "yiisoft/yii2-composer": true
        }
    }
}
